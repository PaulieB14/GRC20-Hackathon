{
  "name": "pinellas-county-grc20-publisher",
  "version": "1.0.0",
  "description": "GRC-20 data publisher for Pinellas County permits and deed transfers",
  "type": "module",
  "module": "src/deploy-space.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/main.js",
    "transform:permits": "node dist/transformPermits.js",
    "transform:deeds": "node dist/transformDeeds.js",
    "publish": "node dist/publish.js",
    "publish:accepted": "node dist/publish-accepted.js",
    "governance:accept": "node dist/create-governance-edit.js",
    "update:knowledge-graph": "node dist/update-knowledge-graph.js",
    "overwrite:with-relations": "node dist/overwrite-with-relations.js",
    "verify:relations": "node dist/verify-relations.js",
    "update:all-entities": "./update-all-entities.sh",
    "lint": "biome check --write --unsafe",
    "format": "biome check --write --unsafe"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@bufbuild/protobuf": "^1.9.0",
    "@changesets/cli": "^2.27.12",
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/providers": "^5.6.8",
    "@fast-csv/parse": "^5.0.2",
    "@graphprotocol/grc-20": "^0.6.4",
    "@types/csv-parse": "^1.1.12",
    "@types/uuid": "^9.0.8",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.7",
    "effect": "^3.12.11",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "ipfs-http-client": "^60.0.1",
    "js-sha3": "^0.9.3",
    "node-fetch": "^3.3.2",
    "position-strings": "^2.0.1",
    "puppeteer": "^24.3.0",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.0",
    "viem": "^1.16.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.31.0",
    "@bufbuild/protoc-gen-es": "^1.9.0",
    "@types/express": "^5.0.0",
    "@types/node": "^20.8.7",
    "typescript": "^5.4.5"
  }
}
